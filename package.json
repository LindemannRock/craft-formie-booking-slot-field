{
  "name": "formie-booking-slot-field",
  "version": "1.0.0",
  "description": "Build tools for Formie Booking Slot Field plugin",
  "scripts": {
    "minify": "npm run minify:css && npm run minify:js",
    "minify:css": "cleancss -o src/web/assets/field/booking-slot.min.css src/web/assets/field/booking-slot.css",
    "minify:js": "terser src/web/assets/field/booking-slot.js -o src/web/assets/field/booking-slot.min.js -c -m"
  },
  "dependencies": {
    "flatpickr": "^4.6.13"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "terser": "^5.31.6"
  }
}
